{
  "name": "imkdw-dev",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint -- --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "clean-repo": "find . -name \"node_modules\" -type d -prune -exec rm -rf {} + && find . -name \"dist\" -type d -prune -exec rm -rf {} + && find . -name \"tsconfig.tsbuildinfo\" -type f -exec rm -f {} +",
    "api": "pnpm -F api",
    "blog": "pnpm -F blog",
    "ui": "pnpm -F ui"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">=22"
  }
}
